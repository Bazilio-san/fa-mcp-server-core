{
  "name": "fa-mcp-sdk",
  "productName": "FA MCP SDK",
  "version": "0.2.14",
  "description": "Core infrastructure and templates for building Model Context Protocol (MCP) servers with TypeScript",
  "type": "module",
  "main": "dist/core/index.js",
  "types": "dist/core/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/core/**/*",
    "README.md"
  ],
  "scripts": {
    "start": "npm run template:start",
    "build": "tsc",
    "clean": "rimraf dist",
    "cb": "npm run clean && npm run build",
    "dev": "tsc --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "ci": "node --no-deprecation ./scripts/npm/run.js",
    "reinstall": "node --no-deprecation ./scripts/npm/run.js reinstall",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run cb",
    "template:start": "node dist/template/start.js",
    "template:stdio": "node dist/template/start.js stdio",
    "token-gen": "node dist/core/token/gen-token-app/gen-token-server.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "typescript",
    "server",
    "framework",
    "template",
    "claude",
    "ai",
    "api",
    "prompts",
    "tools",
    "resources"
  ],
  "author": {
    "name": "Viacheslav Makarov",
    "email": "npmjs@bazilio.ru"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bazilio-san/fa-mcp-sdk.git"
  },
  "homepage": "https://github.com/Bazilio-san/fa-mcp-sdk#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "af-consul-ts": "^4.0.13",
    "af-db-ts": "^3.0.23",
    "af-logger-ts": "^0.0.24",
    "chalk": "^5.6.2",
    "config": "^4.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "pgvector": "^0.2.1",
    "rate-limiter-flexible": "^8.2.1"
  },
  "optionalDependencies": {
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/config": "^3.3.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/mssql": "^9.1.8",
    "@types/node": "^24.10.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "rimraf": "^6.1.0",
    "typescript": "^5.9.3"
  }
}
